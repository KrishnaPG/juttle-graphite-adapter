{
  "name": "juttle-graphite-adapter",
  "version": "0.3.1",
  "description": "Juttle adapter for Graphite",
  "keywords": [
    "juttle",
    "adapter",
    "graphite"
  ],
  "homepage": "https://github.com/juttle/juttle-graphite-adapter",
  "bugs": "https://github.com/juttle/juttle-graphite-adapter/issues",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "repository": "juttle/juttle-graphite-adapter",
  "scripts": {
    "lint": "npm run jshint & npm run style",
    "jshint": "jshint lib/*.js test/*.js",
    "shrinkwrap": "npm-shrinkwrap",
    "style": "jscs lib/*.js test/*.js",
    "test": "eslint **/*.js && node_modules/.bin/istanbul cover -x 'lib/parser.js' node_modules/.bin/_mocha",
    "check-coverage": "node_modules/.bin/istanbul check-coverage"
  },
  "dependencies": {
    "bluebird": "^3.0.5",
    "isomorphic-fetch": "^2.2.0",
    "lazy-socket": "0.0.4",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "bluebird-retry": "^0.5.2",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "istanbul": "^0.4.2",
    "jscs": "^2.6.0",
    "jshint": "^2.9.1-rc1",
    "mocha": "^2.3.4",
    "uuid": "^2.0.1"
  },
  "peerDependencies": {
    "juttle": ">=0.4.0"
  },
  "engines": {
    "node": ">=4.2.0",
    "npm": ">=2.14.7"
  }
